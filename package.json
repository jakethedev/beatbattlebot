{
  "name": "beatbattlebot",
  "version": "1.10.4",
  "description": "Discord.js bot for organizing and handling beat battle submissions on music servers",
  "main": "bot.js",
  "scripts": {
    "start": "npm run start:bot",
    "start:bot": "nodemon bot.js",
    "start:server": "nodemon server.js",
    "start:ui": "echo ERR: there is no UI project yet",
    "starttracedep": "node --trace-deprecation bot.js",
    "devbot": "nodemon bot.js",
    "test": "jest --config=./.jestconfig.js",
    "testcov": "jest --coverage --config=./.jestconfig.js",
    "testdocs": "echo Check out the jest docs at https://jestjs.io/docs",
    "testclearcache": "jest --clearCache",
    "testverbose": "jest --expand --config=./.jestconfig.js",
    "testwatch": "jest --watchAll --config=./.jestconfig.js",
    "build": "echo Placeholder command for compiling libs into a bot.js, server.js, and possibly site/index.html+app.js+styles.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakethedev/beatbattlebot"
  },
  "keywords": [
    "music competition",
    "beat battle",
    "feedback",
    "music",
    "organizer",
    "link submission",
    "discord",
    "bot"
  ],
  "author": "jakethedev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakethedev/beatbattlebot/issues"
  },
  "homepage": "https://github.com/jakethedev/beatbattlebot#readme",
  "dependencies": {
    "dayjs": "^1.11.7",
    "discord.js": "^13.15.1",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^3.0.2"
  }
}
